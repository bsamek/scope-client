{
  "name": "scout-client",
  "description": "A client for scout-server that works in the browser or a server.",
  "version": "0.0.6",
  "author": "Lucas Hrabovsky <lucas@mongodb.com> (http://imlucas.com)",
  "homepage": "http://github.com/10gen/scout",
  "repository": {
    "type": "git",
    "url": "git://github.com/10gen/scout.git"
  },
  "dependency-check": {
    "ignore": [
      "mongodb-datasets"
    ]
  },
  "scripts": {
    "docs": "gulp docs",
    "start": "zuul --local 3001 --open -- test/*.test.js",
    "test": "zuul --phantom -- test/*.test.js",
    "check": "mongodb-js-precommit",
    "start-server": "node bin/start-server.js",
    "stop-server": "node bin/stop-server.js",
    "prestart": "npm run start-server",
    "poststart": "npm run stop-server",
    "pretest": "npm run start-server",
    "posttest": "npm run stop-server"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "event-stream": "^3.3.1",
    "mongodb-extended-json": "^1.5.0",
    "mongodb-ns": "^1.0.0",
    "raf": "^3.1.0",
    "socket.io-client": "^1.3.6",
    "socket.io-stream": "0.6.1",
    "superagent": "^1.3.0"
  },
  "devDependencies": {
    "backbone": "^1.2.2",
    "del": "^1.2.1",
    "dox": "^0.8.0",
    "ecstatic": "^0.8.0",
    "eslint-config-mongodb-js": "^0.1.5",
    "gulp": "^3.9.0",
    "gulp-util": "^3.0.6",
    "handlebars": "^3.0.3",
    "lodash.range": "^3.0.1",
    "mocha": "^2.2.5",
    "mongodb-datasets": "^0.1.5",
    "mongodb-js-precommit": "^0.1.4",
    "mongodb-runner": "mongodb-js/runner",
    "phantomjs": "^1.9.18",
    "phantomjs-polyfill": "0.0.1",
    "pre-commit": "^1.1.1",
    "scout-server": "http://bin.mongodb.org/js/scout-server/v0.1.0/scout-server-0.1.0.tar.gz",
    "through2": "^2.0.0",
    "zuul": "^3.3.1"
  }
}
